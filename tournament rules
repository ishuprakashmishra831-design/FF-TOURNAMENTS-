CREATE TABLE `if0_39734744_gametournament`.`Free Fire Tournament` ( `user name` VARCHAR(15) CHARACTER SET utf8mb3 COLLATE utf8mb3_general_ci NULL COMMENT 'Ye column user ka name bharne ke liye hai ' , `In game name` VARCHAR(15) CHARACTER SET utf8mb3 COLLATE utf8mb3_general_ci NULL COMMENT 'Ye column user ki gaming id name ke liye hai ' , `Uid` INT(15) NULL AUTO_INCREMENT COMMENT 'Ye column gaming id ki uid ke liye hai ' , `mobile number` INT(10) NULL AUTO_INCREMENT COMMENT 'Ye column user ki mobile number ke liye hai' , PRIMARY KEY (`Uid `, `mobile number `)) ENGINE = InnoDB CHARSET=utf8mb3 COLLATE utf8mb3_general_ci COMMENT = 'Ye site ek gaming tournament organized krti hai .';
